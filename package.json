{
  "name": "capturekit",
  "version": "1.0.9",
  "description": "Record the screen on macOS with ScreenCaptureKit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyanshunegi/ElectronCaptureKit.git"
  },
  "main": "./index.cjs",
  "module": "./index.js",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js"
    }
  },
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "swift build --configuration=release --arch arm64 --arch x86_64 && mv .build/apple/Products/Release/capturekit ."
  },
  "files": [
    "index.js",
    "index.cjs",
    "index.mjs",
    "Recorder.js",
    "capturekit",
    "index.d.ts"
  ],
  "dependencies": {
    "execa": "^5.1.1"
  },
  "keywords": [
    "macos",
    "screen",
    "recorder",
    "capture",
    "video"
  ]
}